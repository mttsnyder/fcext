{
    "manifest_version": 2,
    "name": "Full Circle Simple Practice Chrome Extension",
    "description": "This extension does stuff for people",
    "version": "1.0",
    "homepage_url": "http://www.fullcirclewnc.org",
    "browser_action": {
        "default_icon": "favicon.png",
        "default_popup": "popup.html",
        "default_title": "FC Simple"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": false,
        "js":      ["jquery.min.js","content.js","rocketfun.js","inject.js"]
    }],
    "permissions": [
        "https://*/"
    ],
    "content_security_policy": "script-src script-src-elem 'self' 'unsafe-eval' https://www.gstatic.com/; object-src 'self'"
,
    "web_accessible_resources": [
        "favicon.png",
        "FC.jpg",
        "MetallophileSp8-Light.ttf",
        "report.html",
        "report2.html",
        "wreport.html",
        "inject.js"

    ]
}
